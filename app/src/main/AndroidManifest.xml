<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tourguide">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/jhb"
        android:label="@string/app_name"
        android:roundIcon="@drawable/jhb"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="Welcome To Johhanesburg"
            android:screenOrientation="portrait" />
        <activity android:name=".RestaurantsActivity" />
        <activity android:name=".EntertainmentActivity" />
        <activity android:name=".ParksActivity" />
        <activity android:name=".SchoolsActivity"/>
            <activity android:name=".TourDataActivity"/>
                <activity android:name=".ViewTourGuideActivity"/>
    </application>

</manifest>